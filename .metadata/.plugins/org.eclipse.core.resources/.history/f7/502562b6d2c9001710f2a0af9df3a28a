public class SendingEmail 
{
	private JavaMailSender mailsender;
	 String sendEmail()
	 {
		 String status = null;
		 try {

			 MimeMessage message = mailsender.createMimeMessage();
		} catch (Exception e) {
			e.printStackTrace();
		}
	 }
}
