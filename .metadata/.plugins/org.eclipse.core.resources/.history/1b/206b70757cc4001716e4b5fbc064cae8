package com.BridgeLabz.LoginForm.dao;

import com.BridgeLabz.LoginForm.model.User;

public class Validate 
{
	public String validateData(User user)
	{
		String result = null;
		if(user == null)
		{
			result = "All Fields Required";
		}
		if(user.getEmail() == null)
		{
			result = "All Fields Required";
		}
		
		return result;
	}
}
